<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buzztec">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/buzz_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BuzzTec">
        <activity android:name=".UpdateDelete.AgendaActivityUD" />
        <activity android:name=".UpdateDelete.AtividadeActivityUD" />
        <activity android:name=".UpdateDelete.ClienteActivityUD" />
        <activity android:name=".UpdateDelete.ConsultorActivityUD" />
        <activity android:name=".UpdateDelete.ServicoActivityUD" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".LocalizacaoActivity"
            android:label="@string/title_activity_localizacao" />
        <activity android:name=".SobreActivity" />
        <activity android:name=".ConsultaActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".AgendaActivity" />
        <activity android:name=".ServicoActivity" />
        <activity android:name=".AtividadeActivity" />
        <activity android:name=".ClienteActivity" />
        <activity android:name=".ConsultorActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>